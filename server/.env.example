# Server Environment Variables
# Copy this file to .env and fill in your actual values

# MongoDB Configuration
# For development: use local MongoDB instance
MONGODB_URI=mongodb://localhost:27017
# For production: use MongoDB Atlas connection string
MONGODB_ATLAS_URI=mongodb+srv://username:password@cluster.mongodb.net/
MONGODB_DB_NAME=vhybZ-dev

# Google OAuth Configuration
# Get these from Google Cloud Console
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Server Configuration
PORT=8000
NODE_ENV=development

# Production URLs (auto-detected, no need to set manually)
# DENO_DEPLOYMENT_ID=auto-set-by-deno-deploy
# GITHUB_ACTIONS=auto-set-by-github-actions